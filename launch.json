{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "FastAPI: Uvicorn",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "main:app",
          "--host", "0.0.0.0",
          "--port", "8000",
          "--reload"
        ],
        "env": {
          "SSM_PARAM_NAME": "/auth/token",
          "SQS_QUEUE_URL": "https://sqs.us-east-1.amazonaws.com/123456789012/my-queue"
        },
        "jinja": true
      }
    ]
  }